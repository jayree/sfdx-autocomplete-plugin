{
  "name": "sfdx-autocmplt",
  "description": "autocomplete plugin for sfdx",
  "version": "2.3.2",
  "author": "jayree @jayree",
  "bugs": "https://github.com/jayree/sfdx-autocomplete-plugin/issues",
  "main": "lib/index.js",
  "dependencies": {
    "@oclif/core": "^1.8.0",
    "@salesforce/command": "^5.1.0",
    "@salesforce/core": "^3.16.0",
    "debug": "^4.3.4",
    "fs-extra": "^10.1.0",
    "lodash": "^4.17.21",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@oclif/plugin-command-snapshot": "^3",
    "@oclif/plugin-help": "^5.1.12",
    "@salesforce/dev-config": "^3.0.1",
    "@salesforce/dev-scripts": "^2.0.2",
    "@salesforce/prettier-config": "^0.0.2",
    "@types/chai": "^4.3.1",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "oclif": "^3.0.1",
    "patch-package": "^6.4.7",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "autocomplete"
  ],
  "homepage": "https://github.com/jayree/sfdx-autocomplete-plugin",
  "keywords": [
    "sfdx-plugin",
    "sfdx",
    "autocomplete"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "additionalHelpFlags": [
      "-h"
    ],
    "bin": "sfdx",
    "hooks": {
      "update": "./lib/hooks/recache",
      "recache": "./lib/hooks/recache"
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot"
    ],
    "topics": {
      "autocmplt": {
        "description": "display autocomplete installation instructions"
      }
    }
  },
  "repository": "jayree/sfdx-autocomplete-plugin",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "postcompile": "oclif readme",
    "postinstall": "patch-package",
    "postpack": "shx rm -f oclif.manifest.json",
    "postpublish": "pinst --enable",
    "posttest": "yarn lint && yarn test:deprecation-policy",
    "prepack": "sf-prepack",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:deprecation-policy": "./bin/run snapshot:compare",
    "version": "oclif readme"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
