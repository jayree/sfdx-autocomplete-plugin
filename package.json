{
  "name": "sfdx-autocmplt",
  "description": "autocomplete plugin for sfdx and sf CLI",
  "version": "2.5.2",
  "author": "jayree @jayree",
  "bugs": "https://github.com/jayree/sfdx-autocomplete-plugin/issues",
  "type": "module",
  "dependencies": {
    "@oclif/core": "^2.0.6",
    "@salesforce/core": "^3.32.13",
    "@salesforce/sf-plugins-core": "^1.22.1",
    "chalk": "^5.2.0",
    "debug": "^4.3.4",
    "fs-extra": "^11.1.0",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@oclif/plugin-command-snapshot": "^3.2.18",
    "@oclif/plugin-help": "^5.1.23",
    "@oclif/test": "^2.2.21",
    "@salesforce/dev-config": "^3.1.0",
    "@salesforce/dev-scripts": "^3.1.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "chai": "^4.3.7",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-salesforce": "^1.1.0",
    "eslint-config-salesforce-typescript": "^1.1.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sf-plugin": "^1.6.3",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "oclif": "^3.4.3",
    "patch-package": "^6.5.1",
    "pinst": "^3.0.0",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "/bin/is-sfdx.js",
    "/schemas"
  ],
  "homepage": "https://github.com/jayree/sfdx-autocomplete-plugin",
  "keywords": [
    "sfdx-plugin",
    "sfdx",
    "sf",
    "autocomplete"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "additionalHelpFlags": [
      "-h"
    ],
    "bin": "sfdx",
    "hooks": {
      "update": "./lib/hooks/recache",
      "recache": "./lib/hooks/recache"
    },
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot"
    ],
    "topics": {
      "autocmplt": {
        "description": "display autocomplete installation instructions"
      }
    },
    "info": {
      "releasenotes": {
        "distTagUrl": "https://registry.npmjs.org/-/package/sfdx-autocmplt/dist-tags",
        "releaseNotesPath": "https://github.com/jayree/sfdx-autocomplete-plugin/tree/main",
        "releaseNotesFilename": "CHANGELOG.md"
      }
    }
  },
  "repository": "jayree/sfdx-autocomplete-plugin",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile && oclif readme",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "_postinstall": "node ./bin/is-sfdx.js || (patch-package && (is-ci || husky install))",
    "postpack": "pinst --enable && shx rm -f oclif.manifest.json",
    "prepack": "sf-prepack && pinst --disable",
    "test": "sf-compile-test && sf-test && yarn lint && yarn test:deprecation-policy && yarn test:json-schema",
    "test:deprecation-policy": "./bin/dev.js snapshot:compare",
    "test:json-schema": "./bin/dev.js schema:compare",
    "version": "oclif readme"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
