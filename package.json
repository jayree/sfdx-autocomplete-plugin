{
  "name": "sfdx-autocmplt",
  "description": "autocomplete plugin for sfdx",
  "version": "2.3.1",
  "author": "jayree @jayree",
  "bugs": "https://github.com/jayree/sfdx-autocomplete-plugin/issues",
  "dependencies": {
    "@oclif/core": "^1.6.4",
    "@salesforce/command": "^5.0.4",
    "@salesforce/core": "^3.12.1",
    "chalk": "^4.1.2",
    "debug": "^4.3.4",
    "fs-extra": "^10.0.1",
    "lodash": "^4.17.21",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/semantic-release": "^3.1.5",
    "@oclif/test": "^2.1.0",
    "@salesforce/dev-config": "^3.0.1",
    "@salesforce/dev-scripts": "^2.0.1",
    "@salesforce/prettier-config": "^0.0.2",
    "@types/chai": "^4.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "chai": "^4.3.6",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.8",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^38.1.6",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "mocha": "^9.2.2",
    "nock": "13.2.4",
    "nyc": "^15.1.0",
    "oclif": "^2.6.3",
    "patch-package": "6.4.7",
    "pinst": "^3.0.0",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "sinon": "^13.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.7.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "4.6.3",
    "xunit-file": "^2.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/oclif.manifest.json",
    "autocomplete",
    "/bin/per-env.js",
    "yarn.lock"
  ],
  "homepage": "https://github.com/jayree/sfdx-autocomplete-plugin",
  "keywords": [
    "sfdx-plugin",
    "sfdx",
    "autocomplete"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "hooks": {
      "update": "./lib/hooks/recache",
      "recache": "./lib/hooks/recache"
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "jayree/sfdx-autocomplete-plugin",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "compile": "sf-compile",
    "format": "sf-format",
    "lint": "sf-lint",
    "lint-fix": "sf-lint --fix",
    "postcompile": "oclif manifest && oclif readme",
    "postinstall": "patch-package",
    "postpack": "rimraf oclif.manifest.json",
    "postpublish": "pinst --enable",
    "prepack": "sf-prepack",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "version": "oclif manifest && oclif readme"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  }
}
